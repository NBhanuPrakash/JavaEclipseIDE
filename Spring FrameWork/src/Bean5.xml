        <bean id="emp1" class="Spring6.Employee">
        <property name="id" value="1001"></property>
        <property name="empString" value="Ganesh"></property>
        </bean>
        
        <bean id="emp2" class="Spring6.Employee">
        <property name="id" value="1002"></property>
        <property name="empString" value="Ramesh"></property>
        </bean>

</beans>